{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "JSON-LD",
  "type": "object",
  "required": [
    "@context",
    "@graph"
  ],
  "additionalProperties": false,
  "properties": {
    "@context": {
      "title": "JSON-LD Context",
      "type": "string",
      "const": "https://frogcat.github.io/ndl-isil-json/ndl-isil.context.json"
    },
    "@graph": {
      "title": "JSON-LD Graph",
      "type": "array",
      "minItems": 7570,
      "maxItems": 7570,
      "items": {
        "type": "object",
        "required": [
          "@id",
          "name",
          "classification",
          "originalName",
          "@type",
          "hasSite",
          "identifierISIL"
        ],
        "additionalProperties": false,
        "properties": {
          "@id": {
            "title": "@id",
            "type": "string"
          },
          "url": {
            "title": "url",
            "type": "string"
          },
          "name": {
            "title": "name",
            "type": "object",
            "required": [
              "ja"
            ],
            "additionalProperties": false,
            "properties": {
              "ja": {
                "title": "ja",
                "type": "string"
              },
              "ja-kana": {
                "title": "ja-kana",
                "type": "string"
              },
              "en": {
                "title": "en",
                "type": "string"
              }
            }
          },
          "classification": {
            "title": "classification",
            "type": "string",
            "enum": [
              "http://id.ndl.go.jp/vocab/libtype#PublicLibraries",
              "http://id.ndl.go.jp/vocab/libtype#SpecialLibraries",
              "http://id.ndl.go.jp/vocab/libtype#AcademicLibraries",
              "http://id.ndl.go.jp/vocab/libtype#RelatedOrganizations",
              "http://id.ndl.go.jp/vocab/libtype#SchoolLibraryMediaCenters",
              "http://id.ndl.go.jp/vocab/libtype#NationalLibraries"
            ]
          },
          "originalName": {
            "title": "originalName",
            "type": "string"
          },
          "@type": {
            "title": "@type",
            "type": "string",
            "const": "Organization"
          },
          "hasSite": {
            "title": "hasSite",
            "type": "object",
            "required": [
              "siteAddress"
            ],
            "additionalProperties": false,
            "properties": {
              "siteAddress": {
                "title": "siteAddress",
                "type": "object",
                "required": [
                  "addressCountry",
                  "countryCode"
                ],
                "additionalProperties": false,
                "properties": {
                  "postalCode": {
                    "title": "postalCode",
                    "type": "string"
                  },
                  "addressCountry": {
                    "title": "addressCountry",
                    "type": "string",
                    "const": "Japan"
                  },
                  "countryCode": {
                    "title": "countryCode",
                    "type": "string",
                    "const": "JP"
                  },
                  "areaServed": {
                    "title": "areaServed",
                    "type": "string"
                  },
                  "addressRegion": {
                    "title": "addressRegion",
                    "type": "string"
                  },
                  "addressLocality": {
                    "title": "addressLocality",
                    "type": "string"
                  },
                  "streetAddress": {
                    "title": "streetAddress",
                    "type": "string"
                  }
                }
              },
              "long": {
                "title": "long",
                "type": "string"
              },
              "lat": {
                "title": "lat",
                "type": "string"
              },
              "telephone": {
                "title": "telephone",
                "type": "string"
              },
              "faxNumber": {
                "title": "faxNumber",
                "type": "string"
              },
              "@type": {
                "title": "@type",
                "type": "string",
                "const": "Site"
              }
            }
          },
          "identifierISIL": {
            "title": "identifierISIL",
            "type": "string"
          },
          "sameAs": {
            "oneOf": [
              {
                "title": "sameAs",
                "type": "string"
              },
              {
                "type": "array",
                "minItems": 2,
                "maxItems": 2,
                "items": {
                  "type": "string"
                }
              }
            ]
          },
          "seeAlso": {
            "title": "seeAlso",
            "type": "string"
          },
          "alternateName": {
            "title": "alternateName",
            "type": "string"
          },
          "identifierISNI": {
            "title": "identifierISNI",
            "type": "string"
          }
        },
        "title": "Organization"
      }
    }
  }
}
